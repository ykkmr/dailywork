package day0408;


/**
 *	Singleton Pattern 으로 만들어진 클래스를 사용
 * @author user
 */
public class UseSingleton {

	public static void main(String[] args) {
//		Singleton s = new Singleton(); -> 직접 객체화 불가능
		
		Singleton s = Singleton.getInstance(); //getInstance를 써서 객체를 얻어옴
		Singleton s1 = Singleton.getInstance();

		System.out.println(s + " / " + s1);
	}//main

}//class
